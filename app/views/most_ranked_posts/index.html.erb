<p id="notice"><%= notice %></p>

<h1>Most Ranked Posts</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Posts</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @most_ranked_posts.each do |most_ranked_post| %>
      <tr>
        <td><%= most_ranked_post.user_id %></td>
        <td><%= most_ranked_post.posts_id %></td>
        <td><%= link_to 'Show', most_ranked_post %></td>
        <td><%= link_to 'Edit', edit_most_ranked_post_path(most_ranked_post) %></td>
        <td><%= link_to 'Destroy', most_ranked_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Most Ranked Post', new_most_ranked_post_path %>
